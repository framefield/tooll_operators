        "                                (float)rand.NextDouble() - 0.5f,",
        "                                (float)rand.NextDouble() - 0.5f);",
        "        }",
        "        ",
        "        private void ResetParticles () {",
        "            for(var i=0; i< _particles.Count(); ++i  ) {",
