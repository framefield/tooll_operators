        "          asyncState.ResponseCallback(new HttpWebRequestCallbackState(ex));",
        "        else",
        "          throw;",
        "      }",
        "      finally",
        "      {",
        "        if (requestStream != null)",
        "          requestStream.Close();",
        "      }",
        "    }",
