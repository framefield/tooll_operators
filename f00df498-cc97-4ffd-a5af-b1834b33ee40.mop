        "            BackColorR = 19,",
        "            BackColorG = 20,",
        "            BackColorB = 21,",
        "            BackColorA = 22,",
        "            ColorBias = 23",
        "        }",
        "        //<<< _inputids",
        "        #region Renderer",
        "        public class Renderer : BaseRenderer",
        "        {",
        "            public override void SetupEffect(OperatorPartContext context)",
        "            {",
