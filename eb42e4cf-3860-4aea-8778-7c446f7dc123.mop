    {
      "Name": "UpdateCode",
      "MetaInstanceID": "b19cddb5-52ee-47b4-a2cb-f572841576dd",
      "DefaultValue": {
        "Type": "Text",
        "Value": [
          "//>>> UpdateParticle\r",
          "struct Particle\r",
          "{\r",
          "    float3 Position;\r",
          "    float3 Velocity;\r",
          "    float Size;\r",
          "    float BirthTime;\r",
          "    float TextureRow;\r",
          "};\r",
          "\r",
          "RWStructuredBuffer<Particle> ParticleStates : register (u0);\r",
          "\r",
          "cbuffer ParticleParams\r",
          "{\r",
          "    float4 DeltaTime;\r",
          "    float4 Friction;\r",
          "    float4 Wind;\r",
          "    float4 AttractorPosition;\r",
          "    float4 AttractorStrength;\r",
          "    float4 AttractorRadius;\r",
          "    float4 AttractorDecay;\r",
          "    \r",
          "    float4 NoiseTime;\r",
          "    float4 NoiseAmount;\r",
          "    float4 NoiseScale;    \r",
          "    float4 NoiseSeed;\r",
          "    float4 NoiseRadius;\r",
          "    \r",
          "};\r",
          "\r",
          "\r",
          "[numthreads(512, 1, 1)]\r",
          " float Noise(int x, int seed)\r",
          "{\r",
          "    int n = x + seed*137;\r",
          "    n = (n<<13) ^ n;\r",
          "    return (float)( 1.0 - ( (n * (n * n * 15731 + 789221) + 1376312589) & 0x7fffffff) / 1073741824.0);\r",
          "}\r",
          "\r",
          "[numthreads(512, 1, 1)]\r",
          "float Lerp(float a, float b, float t)\r",
          "{\r",
          "    return a + t * (b - a);\r",
          "}\r",
          "\r",
          "[numthreads(512, 1, 1)]\r",
          "float Fade(float t)\r",
          "{\r",
          "    return t * t * t * (t * (t * 6 - 15) + 10);\r",
          "}\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "[numthreads(512, 1, 1)]\r",
          "float getNoise(float value, float zoom, float noiseTime, int seed, float period)\r",
          "{\r",
          "    value /= zoom;\r",
          "    value += noiseTime;\r",
          "        \r",
          "    float noiseSum = 0;\r",
          "    for(int a=0; a<4; a++) {\r",
          "        float f = (float)pow(2,a); \r",
          "        float amplitude = (float)pow(period ,a);\r",
          "\r",
          "        float v = value*f / zoom;\r",
          "       \r",
          "        noiseSum += Lerp( Noise( (int)v  , (int)seed ),\r",
          "                          Noise( (int)v+1, (int)seed ),\r",
          "                          Fade( v - (float)floor(v) )\r",
          "                    )* amplitude;\r",
          "    }   \r",
          "    return noiseSum;    \r",
          "}\r",
          "\r",
          "float3 getNoiseOffset(float3 pos) \r",
          "{\r",
          "\r",
          "    //float3 offset =float3(0,0,0);\r",
          "    //p+= NoiseOffset;\r",
          "\r",
          "    float3 noise1 = float3(getNoise(pos.x + pos.y -136.3f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ),\r",
          "                           getNoise(pos.x + pos.z + 19.3f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ),\r",
          "                           getNoise(pos.y + pos.z - 10.3f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ));\r",
          "\r",
          "    float3 noise2 = float3(getNoise(pos.x + pos.y -411.23f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ),\r",
          "                           getNoise(pos.x + pos.z +131.73f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ),\r",
          "                           getNoise(pos.y + pos.z - 21.33f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ));\r",
          " \r",
          "    float3 noise3 = float3(getNoise(pos.x + pos.y + 31.23f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ),\r",
          "                           getNoise(pos.x + pos.z + 91.73f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ),\r",
          "                           getNoise(pos.y + pos.z - 13.33f, NoiseScale.x, NoiseTime.x, NoiseSeed.x , NoiseRadius.x ));\r",
          "\r",
          "\r",
          "    // Noise Offset\r",
          "    \r",
          "    //float3 noise1 = (RGBANoise.SampleLevel(samLinear, (p.xz + NoiseTime*10 * float3(0.14,0.53,0.41) ) / NoiseRepeat , 0).xyz - float3(0.5, 0.5, 0.5)) * NoiseAmount;    \r",
          "    //float3 noise2 = (RGBANoise.SampleLevel(samLinear, (p.yz + NoiseTime*6 * float3(0.41,0.31,0.01) ) / NoiseRepeat , 0).xyz - float3(0.5, 0.5, 0.5)) * NoiseAmount;    \r",
          "    //float3 noise3 = (RGBANoise.SampleLevel(samLinear, (p.yx + NoiseTime*9 * float3(0.74,0.15,0.55) ) / NoiseRepeat , 0).xyz - float3(0.5, 0.5, 0.5)) * NoiseAmount;    \r",
          "    float3 noiseCombined = noise1+noise2+noise3;\r",
          "    return noiseCombined;\r",
          "}\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "float3 getParticlePosForTime(float time, Particle p ) \r",
          "{\r",
          "        float normalizedTime = time;//15*(1.0 - pow(1 + Friction.x, -time)); \r",
          "        //float3 currentVelocity = Wind.xyz*time + p.Velocity;\r",
          "        float3 damping = float3(pow(1 + AttractorPosition.x, -time),\r",
          "                                pow(1 + AttractorPosition.y, -time),\r",
          "                                pow(1 + AttractorPosition.z, -time));\r",
          "        //p.Position = float3(damping.x*Wind.x, damping.y*Wind.y, damping.z*Wind.z)*0.5*normalizedTime*normalizedTime + p.Velocity*normalizedTime;\r",
          "        float3 pos = float3(damping.x*Wind.x, \r",
          "                            damping.y*Wind.y, \r",
          "                            damping.z*Wind.z)*0.5*normalizedTime*normalizedTime;\r",
          "                            \r",
          "        //if(abs(NoiseAmount.x) > 0.01f) \r",
          "        //{\r",
          "            //float3 noiseOffset = getNoiseOffset(  p.Position);\r",
          "            float3 noiseOffset = getNoiseOffset( pos );\r",
          "            pos+= noiseOffset * NoiseAmount.x;\r",
          "            return pos;\r",
          "\r",
          "}\r",
          "\r",
          "\r",
          "\r",
          "[numthreads(512, 1, 1)]\r",
          "void CSUpdateParticles(uint3 threadID : SV_DispatchThreadID)\r",
          "{\r",
          "\r",
          "    Particle p = ParticleStates[threadID.x];\r",
          "\r",
          "    float time = DeltaTime.x - p.BirthTime;\r",
          "    if (time > 0)\r",
          "    {\r",
          "\r",
          "        float3 pos1 = getParticlePosForTime(time, p);\r",
          "        float3 pos2 = getParticlePosForTime(time+0.001, p);\r",
          "        \r",
          "        p.Position= pos1;\r",
          "        p.Velocity = (pos1-pos2)*10;\r",
          "\r",
          "    }\r",
          "    ParticleStates[threadID.x] = p;\r",
          "}\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "\r",
          "",
          "//<<< UpdateParticle\r",
          ""
        ]
      },
