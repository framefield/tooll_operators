        "        }",
        "        private Buffer _particleUpdateParametersBuffer;",
        "",
        "        //>>> _inputids",
        "        private enum InputId",
        "        {",
        "            InitCode = 0,",
        "            UpdateCode = 1,",
        "            RenderCode = 2,",
        "            Time = 3,",
        "            Image = 4,",
        "            Count = 5,",
