        "namespace Framefield.Core.IDccf5e5de_d7eb_457e_9501_9e06626d90eb",
        "{",
        "    public class Class_GodRaysFromLight : FXImageFunction",
        "    {",
        "        //>>> _inputids",
        "        private enum InputId",
        "        {",
        "            Code = 0,",
        "            Image = 1,",
        "            Camera = 2,",
        "            Strength = 3,",
        "            Rays = 4,",
