        "        public static void SetState(Type newState) {",
        "            currentState.OnPass();",
        "            Logger.Info(\"StateMachine >> Enter state {0}\", newState);",
        "            currentState = Activator.CreateInstance(newState) as SimpleState;",
        "            currentState.OnEnter();",
        "            StateSwitchTime= currentTime;",
