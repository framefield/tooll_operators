        "",
        "namespace Framefield.Core.IDeac31069_7e4b_47d4_9b3b_7302ecfb2a6b",
        "{",
        "    public class Class_SplineDistort__2_ : FXSourceCodeFunction, IFXSceneSourceCode, IMeshSupplier",
        "    {",
        "        //>>> _inputids",
        "        private enum InputId",
        "        {",
        "            Code = 0,",
        "            Scene = 1,",
        "            RGBANoise = 2,",
        "            uShift = 3,",
        "            MeshOffsetX = 4,",
        "            MeshOffsetY = 5,",
        "            MeshOffsetZ = 6,",
        "            DirectionX = 7,",
