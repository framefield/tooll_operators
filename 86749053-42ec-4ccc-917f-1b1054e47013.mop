        "            DirectionZ = 9,",
        "            DirectionRegionUStep = 10,",
        "            DirectionRegionStepSmooth = 11,",
        "            NoiseAmount = 12,",
        "            NoiseRegionUStep = 13,",
        "            NoiseRegionStepSmooth = 14,",
        "            NoiseTime = 15,",
        "            NoiseScale = 16,",
        "            SpinRegionUStep = 17,",
